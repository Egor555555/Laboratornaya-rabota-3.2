//Дано дерево поиска и указатель корень дерева P1. 
//Необходимо вывести минимальное значение в дереве. Решение должно иметь сложность по времени исполнения T(n) = O(log n), где n - число вершин в дереве.
#include "pt4.h"
using namespace std;
PNode Search(PNode Tree,int Data)
{
    if (!Tree) return NULL; // ключ не найден
    if (Data < Tree->Data) // искать в поддеревьях
        return Search(Tree->Left, Data);
}


void Solve()
{
    Task("TreeWork7");
    PNode ptr;
    pt >> ptr;
    Search(ptr,ptr->Data);
    pt << ptr->Data;
}
